<configs>
	<set xpath="/Sounds/SoundDataNode[@name='Auger_Idle']/AudioClip[@ClipName='Sounds/Weapons/Motorized/Auger/auger_idle']/@ClipName">#@modfolder:Resources/toolSounds.unity3d?auger_idle</set>
	<set xpath="/Sounds/SoundDataNode[@name='Auger_Fire_Start']/AudioClip[@ClipName='Sounds/Weapons/Motorized/Auger/Auger_Fire_Start']/@ClipName">#@modfolder:Resources/toolSounds.unity3d?auger_fire_start</set>
	<set xpath="/Sounds/SoundDataNode[@name='Auger_Fire_Start']/AudioClip[@ClipName='Sounds/Weapons/Motorized/Auger/auger_fire_lp']/@ClipName">#@modfolder:Resources/toolSounds.unity3d?auger_fire_lp</set>
	<set xpath="/Sounds/SoundDataNode[@name='Auger_Fire_Start']/AudioClip[@ClipName='Sounds/Weapons/Motorized/Auger/auger_fire_end']/@ClipName">#@modfolder:Resources/toolSounds.unity3d?auger_fire_end</set>

  <append xpath="/Sounds">
    <!--UH-60 Helicopter - BEGIN-->
    <SoundDataNode name="UH60_start">
      <AudioSource name="Sounds/AudioSource_Vehicle"/>
      <Noise ID="2" noise="11" time="3" muffled_when_crouched="0.5"/>
      <AudioClip ClipName="#@modfolder:Resources/UH60sounds.unity3d?heli_start"/>
      <LocalCrouchVolumeScale value="1.0"/>
      <CrouchNoiseScale value="0.5"/>
      <NoiseScale value="1"/>
      <MaxVoices value="10"/>
      <MaxRepeatRate value="0.01"/>
    </SoundDataNode>

    <SoundDataNode name="UH60_stop">
      <AudioSource name="Sounds/AudioSource_Vehicle"/>
      <Noise ID="2" noise="11" time="3" muffled_when_crouched="0.5"/>
      <AudioClip ClipName="#@modfolder:Resources/UH60sounds.unity3d?heli_stop"/>
      <LocalCrouchVolumeScale value="1.0"/>
      <CrouchNoiseScale value="0.5"/>
      <NoiseScale value="1"/>
      <MaxVoices value="10"/>
      <MaxRepeatRate value="0.01"/>
    </SoundDataNode>

    <SoundDataNode name="UH60_flying">
      <AudioSource name="Sounds/AudioSource_Vehicle"/>
      <AudioClip ClipName="#@modfolder:Resources/UH60sounds.unity3d?heli_flying" Loop="true"/>
      <LocalCrouchVolumeScale value="1.0"/>
      <CrouchNoiseScale value="0.5"/>
      <NoiseScale value="1"/>
      <MaxVoices value="10"/>
      <MaxRepeatRate value="0.01"/>
    </SoundDataNode>

    <SoundDataNode name="UH60_flying2">
      <AudioSource name="Sounds/AudioSource_Vehicle"/>
      <AudioClip ClipName="#@modfolder:Resources/UH60sounds.unity3d?heli_flying" Loop="true"/>
      <LocalCrouchVolumeScale value="1.0"/>
      <CrouchNoiseScale value="0.5"/>
      <NoiseScale value="1"/>
      <MaxVoices value="10"/>
      <MaxRepeatRate value="0.01"/>
    </SoundDataNode>
    <!--UH-60 Helicopter - END-->
  </append>
</configs>